<%- include ("./partials/header") %>

<div class="card result-card">
    <div class="card-body">
        <div>
            <ul>
                SumQuiz
                    <% for(let i = 0; i < user.sumQuiz.length; i++) 
                    { %>
                        <li><%= user.sumQuiz[i].score %> - <%= user.sumQuiz[i].date %></li>
                    <% } %>
                MinusQuiz
                    <% for(let i = 0; i < user.minusQuiz.length; i++) 
                    { %>
                        <li><%= user.minusQuiz[i].score %> - <%= user.minusQuiz[i].date %></li>
                    <% } %>
            </ul>
        </div>
        <div class="home-button">
            <a href="/" class="card-link">
                <button type="submit" class="btn btn-outline-warning">Home</button>
            </a>
        </div>
    </div>
</div>

<%- include ("./partials/footer") %>